  "scripts": {
    "test": "npm run test-client && npm run test-server && npm run test-test",
    "test-client": "NODE_ENV=test NODE_PATH=test:client TEST_ROOT=client test/runner.js",
    "test-server": "NODE_ENV=test NODE_PATH=test:server:client TEST_ROOT=server test/runner.js",
    "test-test": "NODE_ENV=test NODE_PATH=test:client TEST_ROOT=test test/runner.js",
    "test-client:watch": "nodemon -e js,jsx --exec npm run test-client",
    "test-server:watch": "nodemon -e js,jsx --exec npm run test-server",
    "test-test:watch": "nodemon -e js,jsx --exec npm run test-test",
    "lint": "bin/run-lint .",
    "css-lint": "stylelint 'client/**/*.scss' --syntax scss",
    "start": "cross-env NODE_PATH=$NODE_PATH:server:client:. CALYPSO_ENV=development NODE_ENV=development \"npm run build && node build/bundle.js\"",
    "build": "run-p generate-proptypes-index generate-components-usage-stats build-server generate-devdocs-index build-css",
    "generate-proptypes-index": "node server/devdocs/bin/generate-proptypes-index.js client/**/index.js client/**/index.jsx client/**/example.jsx !**/test/**",
    "generate-components-usage-stats": "node server/devdocs/bin/generate-components-usage-stats.js client/**/*.js client/**/*.jsx !**/docs/** !**/test/** !**/docs-example/**",
    "build-server": "mkdirp build && webpack --display-error-details --config webpack.config.node.js",
    "generate-devdocs-index": "node server/devdocs/bin/generate-devdocs-index **/*.md !build/** !.git/** !node_modules/** !public/**",
    "build-css": "run-p build-css:*",
    "build-css:style": "node-sass --include-path client assets/stylesheets/style.scss public/style.css && npm run autoprefixer -- public/style.css && rtlcss public/style.css public/style-rtl.css",
    "build-css:debug": "node-sass --include-path client --source-map \"public/style-debug.css.map\" assets/stylesheets/style.scss public/style-debug.css && npm run autoprefixer -- public/style-debug.css",
	"build-css:editor": "node-sass --include-path client assets/stylesheets/editor.scss public/editor.css && npm run autoprefixer -- public/editor.css",
	"build-css:directly": "node-sass --include-path client assets/stylesheets/directly.scss public/directly.css && npm run autoprefixer -- public/directly.css",
    "autoprefixer": "postcss -r --use autoprefixer --autoprefixer.browsers \"last 2 versions, > 1%, Safari >= 8, iOS >= 8, Firefox ESR, Opera 12.1\""
  },
